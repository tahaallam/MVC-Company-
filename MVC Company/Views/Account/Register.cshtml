
@model RegisterDto
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/Auth_Layout.cshtml";
}
<div asp-validation-summary="All"></div>
<div class="main-agileinfo">
	<div class="agileits-top">
		<form action="#" method="post">
			
			<input class="text" asp-for="FirstName" placeholder="First Name" required=""/>
			<span asp-validation-for="FirstName"></span>
			<input class="text" asp-for="LastName" placeholder="LastName" required=""/>
			<span asp-validation-for="LastName"></span>
			<input class="text" asp-for="Email" placeholder="Email" required=""/>
			<span asp-validation-for="Email"></span>
			<input class="text" asp-for="Password" placeholder="Password" required=""/>
			<span asp-validation-for="Password"></span>
			<input class="text" asp-for="ConfirmPassword" placeholder="Confirm Password" required=""/>
			<span asp-validation-for="ConfirmPassword"></span>
			<div class="wthree-text">
				<label class="anim">
					<input asp-for="IsAgree" class="checkbox" >
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Do You have an Account? <a asp-action="Login"> Login Now!</a></p>
	</div>
</div>
@section validationScript{
	<partial name="_ValidationScriptPartial" >
}
